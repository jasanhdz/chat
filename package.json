    "jsonwebtoken": "^9.0.2",
    "mdi-vue3": "^7.4.47",
    "mongoose": "^8.8.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "uuid": "^11.0.2",
    "vue": "^3.5.12",
    "vue-router": "4",
    "vuex": "^4.0.2"
  },
